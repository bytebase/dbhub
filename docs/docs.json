{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "fonts": {
    "family": "DM Sans"
  },
  "name": "DBHub",
  "colors": {
    "primary": "#FD8D31",
    "light": "#DCFCE7",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "pages": [
          "index",
          "installation",
          "quickstart"
        ]
      },
      {
        "group": "Database Support",
        "pages": [
          "databases/overview",
          "databases/postgresql",
          "databases/mysql",
          "databases/mariadb",
          "databases/sqlserver",
          "databases/sqlite"
        ]
      },
      {
        "group": "Configuration",
        "pages": [
          "config/dsn",
          "config/multi-database",
          "config/environment",
          "config/ssl",
          "features/ssh-tunnel",
          "features/readonly-mode",
          "features/row-limiting",
          "features/multi-instance"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "type": "github",
      "href": "https://github.com/bytebase/dbhub"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/bytebase/dbhub",
      "x": "https://x.com/Bytebase"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "seo": {
    "metatags": {
      "canonical": "https://dbhub.ai"
    }
  }
}
