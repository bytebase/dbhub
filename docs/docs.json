{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "fonts": {
    "family": "DM Sans"
  },
  "name": "DBHub",
  "colors": {
    "primary": "#FD8D31",
    "light": "#DCFCE7",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "pages": [
          "index",
          "installation",
          "quickstart"
        ]
      },
      {
        "group": "Integrations",
        "pages": [
          "integrations/claude-desktop",
          "integrations/claude-code",
          "integrations/cursor",
          "integrations/vscode"
        ]
      },
      {
        "group": "Configuration",
        "pages": [
          "config/startup-options",
          "config/multi-database"
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "type": "github",
      "href": "https://github.com/bytebase/dbhub"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/bytebase/dbhub",
      "x": "https://x.com/Bytebase"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "errors": {
    "404": {
      "redirect": false
    }
  },
  "seo": {
    "metatags": {
      "canonical": "https://dbhub.ai"
    }
  }
}
